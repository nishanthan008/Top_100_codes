#include <stdio.h>
#include<math.h>
int main() {
  int n,sum=0,i;
  printf("enter the value:");
  scanf("%d",&n);
 for(i=1;i<n;i++)
  {
      if(n%i==0) {
          sum = sum + i;
      }
  }
  if(n==sum) {
      printf("%d is a perfect number", n);
  }
  else
  {
      printf("%d is Not a perfect number", n);
  }
    return 0;
}
