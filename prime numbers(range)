#include <stdio.h>
#include<math.h>
int main() {
  int n,rev,i=0,c=0;
  for(int i=1;i<=100;i++)
  {
      c=0;
      for(int j=1;j<=i;j++)
      {
          if(i%j==0)
          {
              c++;
          }
      }
      if(c==2)
      {
          printf(" %d is a Prime number\n",i);
      }
  }
    return 0;
}
