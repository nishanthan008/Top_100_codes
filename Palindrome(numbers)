#include <stdio.h>
int main() {
  int n,sum=0,rev=0,s,t;
  printf("enter the value:");
  scanf("%d",&n);
  t=n;
  while(n>0)
  {
      rev=n%10;
      sum=(sum*10)+rev;
      n=n/10;
  }
  if(t==sum) {
      printf("%d is a Palindrome", sum);
  }
  else
  {
      printf("%d is Not aPalindrome", sum);
  }
    return 0;
}
