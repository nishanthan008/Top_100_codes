#include <stdio.h>
#include<math.h>
int main() {
  int n1,n2,max,i;
  printf("enter the Number1:");
  scanf("%d",&n1);
    printf("enter the Number1:");
    scanf("%d",&n2);

  if(n1>n2) {
     max=n1;
  }
  else
  {
      max=n2;
  }
  while(1)
  {
      if(max%n1==0 && max%n2==0)
      {
          printf("%d is a LCM of %d and %d",max,n1,n2);
          break;
      }
      max++;
  }
    return 0;
}
