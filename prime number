#include <stdio.h>
#include<math.h>
int main() {
  int n,sum=0,i;
  printf("enter the value:");
  scanf("%d",&n);
 for(i=2;i<=n;i++)
  {
      if(n%i==0) {
          sum++;
      }
  }
  if(sum==1) {
      printf("%d is a prime number", n);
  }
  else
  {
      printf("%d is Not a prime number", n);
  }
    return 0;
}
