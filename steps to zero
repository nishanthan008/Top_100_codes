#include<iostream>
#include<vector>
using namespace std;
int main()
{
    int s=20,count=0;
    cout<<"enter value";
    cin>>s;
    while(s>0) {
        if (s % 2 == 0)
        {
            s = s / 2;
        } else
        {
                s = s - 1;
        }
        count++;
    }
  cout<<count<<endl;
}
